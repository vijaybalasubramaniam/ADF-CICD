{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-adf-vijay"
		},
		"ls_ablob_covidreportingsa18_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa18'"
		},
		"ls_adls_covidreportingdlsa18_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdlsa18'"
		},
		"ls_azure_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azure_sql_covid_db'"
		},
		"ls_adls_covidreportingdlsa18_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdlsa18.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europe_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f4044403-9f31-4882-9400-88a96284392d/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingsa18"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-adf-vijay_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa18_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa18_connectionString')]"
					},
					"ls_adls_covidreportingdlsa18_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdlsa18_accountKey')]"
					},
					"ls_azure_sql_covid_db_connectionString": {
						"value": "[parameters('ls_azure_sql_covid_db_connectionString')]"
					},
					"ls_adls_covidreportingdlsa18_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdlsa18_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europe_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europe_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-reporting-adf-vijay_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-reporting-adf-vijay_ArmTemplate_0')]"
			]
		}
	]
}